  owner        = {garganti},
  presentation = {slides_FM2CitLabGargantini.pdf},
  subject      = {2.Combinatorial testing},
  timestamp    = {2014.12.02},
  urlcommented = {http://dx.doi.org/10.1109/ICSTW.2013.45},
}

